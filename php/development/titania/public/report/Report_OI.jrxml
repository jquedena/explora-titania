	<queryString>
		<![CDATA[select distinct f.cnumpis,e.vnrodoc,h.vdirpre,h.mviadis, b.dpredio,e.idsigma as codcontri
,e.vnombre|| ' ' ||  e.vpatern|| ' ' || e.vmatern as NombreContri,ctipper, e.nestado, e.ntipers, e.ntipper, e.cubigeo, e.cdenomi, e.vdirecc,
       e.vnumero, e.vlote, e.vmanzan, e.vdpto, e.vreferen, e.ctipdoc, e.vnrodoc as DNI, e.cestciv,
       e.vobserv, i.vnombre as NombreCP,c.cclasif as clasificacion
       ,j.vdescri as TipoPredio,k.vdescri as CondicPre,l.vdescri as EstaPredio, m.vdescri as UsoPredio,c.nporcen
,b.vdesint as DEscripcionInstalacion, a.nvalins as ValorInstalacion
from registro.dinstal a
inner join registro.minstal b on a.minstal=b.idsigma
inner join registro.dpredio c on b.dpredio=c.idsigma
inner join registro.mpropie d on c.mpredio=d.mpredio

inner join public.mperson e on d.mperson=e.idsigma
inner join registro.mconstr f on c.idsigma=f.dpredio
inner join registro.dconstr g on f.idsigma=g.mconstr

inner join registro.mpredio h on  d.mpredio=h.idsigma
inner join registro.mpoblad i on h.mpoblad=i.idsigma

inner join public.mconten j on c.ctippre=j.idsigma
inner join public.mconten k on c.ccondic=k.idsigma
inner join public.mconten l on c.cestado=l.idsigma

inner join public.mconten m on c.cusogen=m.idsigma


where c.idsigma=$P{p_dpredio} and g.cperiod=(select max(bx.cperiod) from registro.mconstr ax inner join registro.dconstr bx on ax.idsigma=bx.mconstr where ax.dpredio=$P{p_dpredio})
	order by f.cnumpis;]]>
	</queryString>
